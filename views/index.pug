extends layout

block content
  h1= title
  p Welcome to Northwest Missouri State University
  button(type="button", class="btn btn-secondary", onclick="goFaster()") Press Me
  div(id="faster") Click to Increase Speed
  script.
      var mph = 20;
      var change = 1;
      function goFaster() {
        mph += change;
        change++;
        var div = document.getElementById("faster");
        div.textContent = "MPH " + mph + " with change " + change;
      }
  a(href="/car")  CAR Results